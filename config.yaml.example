bot:
  name: carik-bot
  prefix: '!'
plugins:
  directory: ./plugins
  auto-load: false
butler:
  enabled: true
  name: Carik
  description: Your personal AI assistant
  acknowledge_plugins: true
  include_plugin_context: true
security:
  rate-limit:
    max-requests: 20
    window-seconds: 60
  sandbox:
    enabled: false
    memory-mb: null
  audit:
    enabled: false
    path: null
adapters:
  telegram:
    enabled: true
    token: YOUR_BOT_TOKEN_HERE
  console:
    enabled: false
whitelist:
  enabled: true
  users:
  - YOUR_TELEGRAM_ID
guests:
  enabled: true
  pending: []
  approved: []
